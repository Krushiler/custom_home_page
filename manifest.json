{
  "manifest_version": 2,
  "name": "Custom Home Page",
  "version": "1.0",
  "description": "Adds customizable styled home page to your browser",
  "icons": {
    "16": "src/icons/home_smile16.png",
    "48": "src/icons/home_smile48.png",
    "128": "src/icons/home_smile128.png"
  },
  "chrome_url_overrides" : {
      "newtab": "index.html"
    },
  "permissions": [
      "activeTab"
  ]
}